{
  "name": "levelcredit/tradeline-php",
  "description": "LevelCredit Tradeline SDK for PHP",
  "minimum-stability": "stable",
  "license": "MIT",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Yuriy Borovchuk",
      "email": "yuriy@renttrack.com",
      "homepage": "https://www.levelcredit.com"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:levelcredit/levelcredit-api-php.git"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "ext-json": "*",
    "psr/log": "^1.1",
    "monolog/monolog": "2.*",
    "levelcredit/levelcredit-api-php": "1.*"
  },
  "autoload": {
    "psr-4": {
      "LevelCredit\\Tradeline\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "8.*",
    "vlucas/phpdotenv": "5.*",
    "fakerphp/faker": "^1.10"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload-dev": {
    "psr-4": {
      "LevelCredit\\Tradeline\\Tests\\": "tests/"
    }
  }
}
